{
  "name": "SequenceArtboardNames",
  "description": "Simple sequencing of all artboards within a Sketch Document",
  "license": "MIT",
  "repository": "https://github.com/tbrasington/Sequence-Sketch-Artboards",
  "version": "0.1.2",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Sequence Artboard Names",
    "manifest": "src/manifest.json",
    "main": "sequence-artboard-names.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "Thomas Brasington <tbrasington@gmail.com>"
}
